{
  "name": "@sarast/eslint-config-monorepo",
  "version": "0.0.16",
  "private": true,
  "packageManager": "pnpm@9.6.0",
  "scripts": {
    "prepare": "pnpm -r run stub",
    "lint": "pnpm prepare && eslint . --fix",
    "format": "pnpm prepare && prettier --write .",
    "typecheck": "pnpm -r run typecheck",
    "test": "pnpm -r run test",
    "build": "pnpm -r run build",
    "release": "bumpp -r && pnpm -r publish --access public"
  },
  "devDependencies": {
    "@sarast/eslint-config": "workspace:*",
    "bumpp": "9.4.1",
    "eslint": "8.57.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-prettier": "5.2.1",
    "eslint-plugin-sarast": "link:./packages/eslint-plugin-sarast",
    "prettier": "3.3.3",
    "rimraf": "6.0.1",
    "typescript": "5.4.5"
  },
  "pnpm": {
    "overrides": {
      "@typescript-eslint/rule-tester@eslint": "8.57.0",
      "@typescript-eslint/typescript-estree@typescript": "5.4.5",
      "eslint-plugin-unused-imports@typescript-eslint/eslint-plugin": "7.3.1"
    }
  }
}
