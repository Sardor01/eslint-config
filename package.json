{
  "name": "@sarast/eslint-config",
  "version": "1.5.0",
  "packageManager": "pnpm@9.14.2+sha256.06e65a4965baff6d6097f9c8f75c35f6d420974dbc03d775009056a69edfd271",
  "type": "module",
  "license": "MIT",
  "homepage": "https://github.com/sardor01/eslint-config#readme",
  "bugs": {
    "url": "https://github.com/sardor01/eslint-config/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sardor01/eslint-config.git"
  },
  "files": [
    "dist"
  ],
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": "./dist/index.js",
    "./package.json": "./package.json"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "dev": "eslint-config-inspector --config eslint-inspector.config.ts",
    "build": "pnpm run build:typegen && tsup",
    "build:typegen": "tsx scripts/typegen.ts",
    "build:inspector": "eslint-config-inspector build --config eslint-inspector.config.ts",
    "lint": "eslint --flag unstable_ts_config .",
    "lint:fix": "pnpm run lint --fix",
    "release": "bumpp && pnpm publish",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "pnpm run build"
  },
  "peerDependencies": {
    "eslint": "^9.10.0"
  },
  "dependencies": {
    "@eslint/js": "9.15.0",
    "@eslint/markdown": "6.2.1",
    "@types/eslint": "9.6.1",
    "@unocss/eslint-plugin": "0.64.1",
    "eslint-config-flat-gitignore": "0.3.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-antfu": "2.7.0",
    "eslint-plugin-command": "0.2.6",
    "eslint-plugin-eslint-comments": "3.2.0",
    "eslint-plugin-import-x": "4.4.3",
    "eslint-plugin-jsdoc": "50.5.0",
    "eslint-plugin-jsonc": "2.18.2",
    "eslint-plugin-n": "17.14.0",
    "eslint-plugin-perfectionist": "4.1.2",
    "eslint-plugin-prettier": "5.2.1",
    "eslint-plugin-regexp": "2.7.0",
    "eslint-plugin-unicorn": "56.0.1",
    "eslint-plugin-unused-imports": "4.1.4",
    "eslint-plugin-vue": "9.31.0",
    "eslint-plugin-yml": "1.15.0",
    "globals": "15.12.0",
    "jsonc-eslint-parser": "2.4.0",
    "local-pkg": "0.5.1",
    "prettier": "3.3.3",
    "typescript-eslint": "8.16.0",
    "vue-eslint-parser": "9.4.3",
    "yaml-eslint-parser": "1.2.3"
  },
  "devDependencies": {
    "@eslint/config-inspector": "0.5.6",
    "@sarast/prettier-config": "1.0.1",
    "@types/node": "22.9.4",
    "bumpp": "9.8.1",
    "eslint": "9.15.0",
    "eslint-typegen": "0.3.2",
    "importx": "0.5.0",
    "picocolors": "1.1.1",
    "tsup": "8.3.5",
    "tsx": "4.19.2",
    "typescript": "5.7.2"
  },
  "engines": {
    "node": "^18.18.0 || >=20.0.0"
  },
  "pnpm": {
    "overrides": {
      "is-core-module": "npm:@no-shims/is-core-module"
    }
  },
  "prettier": "@sarast/prettier-config"
}
